{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "Video Streaming Application",
  "version": 2,
  "public": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"

    }

  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "server/**"

    }

  },
  "outputDirectory": "dist/video-streaming"

}
